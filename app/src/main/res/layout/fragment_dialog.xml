<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ui.css.AddTagFragment">


    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="新建/修改标签"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04" />

    <EditText
        android:id="@+id/tagNameEdit"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/qmui_config_color_black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tagImage3"
        app:layout_constraintStart_toStartOf="@+id/tagImage1"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.05" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="输入标签名字"
        app:layout_constraintBottom_toBottomOf="@+id/tagNameEdit"
        app:layout_constraintEnd_toStartOf="@+id/tagImage1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tagNameEdit" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="选择标签颜色"
        app:layout_constraintBottom_toBottomOf="@+id/tagImage4"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/tagImage1" />

    <ImageView
        android:id="@+id/tagImage1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/tag1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tagNameEdit"
        app:layout_constraintVertical_bias="0.00999999" />

    <ImageView
        android:id="@+id/tagImage2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:src="@drawable/tag2"
        app:layout_constraintBottom_toBottomOf="@+id/tagImage1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tagImage1" />

    <ImageView
        android:id="@+id/tagImage3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tag3"
        app:layout_constraintBottom_toBottomOf="@+id/tagImage2"
        app:layout_constraintEnd_toEndOf="@+id/tagImage6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/tagImage6"
        app:layout_constraintTop_toTopOf="@+id/tagImage2" />

    <ImageView
        android:id="@+id/tagImage4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/tag4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tagImage1"
        app:layout_constraintStart_toStartOf="@+id/tagImage1"
        app:layout_constraintTop_toBottomOf="@+id/tagImage1"
        app:layout_constraintVertical_bias="0.42000002" />

    <ImageView
        android:id="@+id/tagImage5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:src="@drawable/tag5"
        app:layout_constraintBottom_toBottomOf="@+id/tagImage4"
        app:layout_constraintEnd_toEndOf="@+id/tagImage2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tagImage2"
        app:layout_constraintTop_toTopOf="@+id/tagImage4" />

    <ImageView
        android:id="@+id/tagImage6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:src="@drawable/tag6"
        app:layout_constraintBottom_toBottomOf="@+id/tagImage5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toEndOf="@+id/tagImage5"
        app:layout_constraintTop_toTopOf="@+id/tagImage5" />

    <ImageView
        android:id="@+id/tick1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/tick"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/tagImage4"
        app:layout_constraintEnd_toEndOf="@+id/tagImage1"
        app:layout_constraintStart_toStartOf="@+id/tagImage1"
        app:layout_constraintTop_toBottomOf="@+id/tagImage1" />

    <ImageView
        android:id="@+id/tick2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/tick"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/tagImage5"
        app:layout_constraintEnd_toEndOf="@+id/tagImage2"
        app:layout_constraintStart_toStartOf="@+id/tagImage2"
        app:layout_constraintTop_toBottomOf="@+id/tagImage2" />

    <ImageView
        android:id="@+id/tick3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/tick"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/tagImage6"
        app:layout_constraintEnd_toEndOf="@+id/tagImage3"
        app:layout_constraintStart_toStartOf="@+id/tagImage3"
        app:layout_constraintTop_toBottomOf="@+id/tagImage3" />

    <ImageView
        android:id="@+id/tick4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/tick"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/cancleAdd"
        app:layout_constraintEnd_toEndOf="@+id/tagImage4"
        app:layout_constraintStart_toStartOf="@+id/tagImage4"
        app:layout_constraintTop_toBottomOf="@+id/tagImage4" />

    <ImageView
        android:id="@+id/tick5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:src="@drawable/tick"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/tick4"
        app:layout_constraintEnd_toEndOf="@+id/tagImage5"
        app:layout_constraintStart_toStartOf="@+id/tagImage5"
        app:layout_constraintTop_toTopOf="@+id/tick4" />

    <ImageView
        android:id="@+id/tick6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:src="@drawable/tick"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/tick5"
        app:layout_constraintEnd_toEndOf="@+id/tagImage6"
        app:layout_constraintStart_toStartOf="@+id/tagImage6"
        app:layout_constraintTop_toTopOf="@+id/tick5" />

    <Button
        android:id="@+id/confirmAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="确定"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tagImage6"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/cancleAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="取消"
        app:layout_constraintBottom_toBottomOf="@+id/confirmAdd"
        app:layout_constraintEnd_toStartOf="@+id/confirmAdd"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/confirmAdd"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>